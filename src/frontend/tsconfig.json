{
  "compilerOptions": {
    "lib": [
      "es6"
    ],
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "pretty": true,
    "target": "es6",
    "resolveJsonModule": true
  }
}
